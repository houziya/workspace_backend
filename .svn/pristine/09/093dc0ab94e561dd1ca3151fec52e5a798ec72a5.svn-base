<div class="row-fluid">
  <div class="span12">
    <h3 class="page-title"> M包明细 <small>My Homepage</small> </h3>
    <ul class="breadcrumb">
      <li> <i class="icon-home"></i> <a href="__APP__/Public/main">Home</a> <i class="icon-angle-right"></i> </li>
      <li><a href="/index.php?s=/Bonus/wallet">M包明细</a></li>
    </ul>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <div class="borecord">
      <h3>互助钱包记录</h3>
      <div class="bonlist">
        <div class="portlet box grey">
          <div class="portlet-title">
            <div class="caption"><i class="icon-list"></i> 互助钱包记录</div>
          </div>
          <div class="portlet-body flip-scroll">
            <table class="table table-hover table-bordered flip-content table-full-width" style=" font-size:12px">
              <thead>
                <tr>
                  <th>编号</th>
                  <th>日期</th>
                  <th>说明</th>
                  <th>原余额</th>
                  <th>+收入/-支出</th>
                  <th>新余额</th>
                </tr>
              </thead>
              <tbody>
                <volist name="cash" id='voo'>
                  <tr >
                    <th>{$voo['code']}{$key|key_num}</th>
                    <th>{$voo['time']|date='Y-m-d H:i:s',###}</th>
                    <th>{$voo['content']}</th>
                    <th>{$voo['old_money']}</th>
                    <th><gt name="voo['money']" value="0">+</gt>
                      {$voo['money']}</th>
                    <th>{$voo['new_money']}</th>
                  </tr>
                </volist>
              </tbody>
            </table>
            <table class="table table-hover table-bordered flip-content  table-full-width" font-size:12px">
              <tr >
                <td>{$bpage}</td>
              </tr>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <div class="borecord">
      <h3>业绩钱包记录</h3>
      <div class="bonlist">
        <div class="portlet box grey">
          <div class="portlet-title">
            <div class="caption"><i class="icon-list"></i> 业绩钱包记录</div>
          </div>
          <div class="portlet-body flip-scroll">
            <table class="table table-hover table-bordered flip-content  table-full-width" font-size:12px">
              <thead>
                <tr>
                  <th>编号</th>
                  <th>日期</th>
                  <th>说明</th>
                  <th>原余额</th>
                  <th>+收入/-支出</th>
                  <th>新余额</th>
                </tr>
              </thead>
              <tbody style="font-size:12px;">
                <volist name="use" id='voo'>
                  <tr >
                    <th>{$voo['code']}{$key|key_num}</th>
                    <th>{$voo['time']|date='Y-m-d H:i:s',###}</th>
                    <th>{$voo['content']}</th>
                    <th>{$voo['old_money']}</th>
                    <th><gt name="voo['money']" value="0">+</gt>
                      {$voo['money']}</th>
                    <th>{$voo['new_money']}</th>
                  </tr>
                </volist>
              </tbody>
            </table>
            <table class="table table-hover table-bordered flip-content  table-full-width" font-size:12px">
              <tr >
                <td>{$spage}</td>
              </tr>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
