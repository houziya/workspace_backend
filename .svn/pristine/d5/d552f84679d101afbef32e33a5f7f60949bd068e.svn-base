<include file="../Public/header" />
<style>
.tab_l3 {
	font-size:14px;
}
</style>
<div class="ncenter_box">
  <div class="accounttitle">
    <h1>机器人管理</h1>
  </div>
  <table width="100%" border="0" align="center" cellpadding="0" cellspacing="1" class="tab" >
    <tr>
      <td height="28" colspan="8" align="center"><table width="100%" border="0" cellspacing="0" cellpadding="5">
          <tr>
            <td width="40%"><table width="70%" border="0" align="center" cellpadding="5" cellspacing="0" class="tab_l3" id="noborder">
                <tr>
                  <td align="left"><table width="100%" border="0" cellspacing="0" cellpadding="0">
                      <tr class="bor">
                        <th width="35%" height="30" align="left">可用人民币账户余额：<span style="color:#F00;">{$fck_rs['agent_gp']}</span></th>
                      </tr>
                      <tr class="bor">
                        <th height="30" align="left">代币持有数量：<span style="color:#F00;">{$fck_rs['live_gupiao']}</span> 个</th>
                      </tr>
                      <tr class="bor">
                        <th height="30" align="left">机器人持有数量：<span style="color:#F00;">{$fck_rs['down_num']}</span> 个</th>
                      </tr>
                      <tr class="bor">
                        <th height="30" align="left">机器人代币收益：<span style="color:#F00;">{$fck_rs['xf_money']}</span> 个</th>
                      </tr>
                    </table></td>
                </tr>
              </table></td>
            <td width="60%"><form action="__URL__/JJRbuyformSave" method="post" name="form1" id="form1" onsubmit="return checkf();">
                <table width="400" border="0" cellpadding="0" cellspacing="0" class="tab5">
                  <tr>
                    <td height="40" align="right">购买价格：</td>
                    <td align="left" style="padding-left:10px;">{$fee_rs['s4']} 人民币</td>
                  </tr>
                  <tr>
                    <td width="30%" height="40" align="right">租用价格：</td>
                    <td width="70%" align="left" style="padding-left:10px;">{$fee_rs['s5']} 人民币</td>
                  </tr>
                  <tr>
                    <td height="40" align="right">类型：</td>
                    <td align="left" style="padding-left:10px;">
                    <eq name="fee_rs['s15']" value="0">
                    <input type="radio" name="s_type" id="radio" value="1" />购买
                    </eq>
                      <input type="radio" name="s_type" id="radio2" value="2" />
                      租用</td>
                  </tr>
                  <tr>
                    <td height="40" align="right">数量：</td>
                    <td align="left" style="padding-left:10px;"><input name="shu" type="text" id="shu"  maxlength="20" value="0" />
                      个</td>
                  </tr>
                  <tr>
                    <td height="40" align="right">三级密码：</td>
                    <td align="left" style="padding-left:10px;"><input name="gp_pwd" type="password" id="gp_pwd" value="" /></td>
                  </tr>
                  <tr>
                    <td height="40" align="right">&nbsp;</td>
                    <td align="left" style="padding-left:10px;"><input type="submit" name="button2" id="button2" value="{:xstr('confirm_2')}" class="btn1" /></td>
                  </tr>
                </table>
              </form></td>
          </tr>
        </table>
        <br />
        <table width="100%" border="0" cellspacing="0" cellpadding="5" class="tab3" id="tb1">
          <tr>
            <td align="center" colspan="6"><span style="font-size:14px; line-height:30px; font-weight:bold">机器人买租记录</span></td>
          </tr>
          <tr>
            <th width="14%" height="28"><span>{:xstr('account')}</span></th>
            <th width="12%"><span>类型</span></th>
            <th width="12%"><span>数量</span></th>
            <th width="12%"><span>总金额</span></th>
            <th width="12%"><span>{:xstr('time')}</span></th>
            <th width="12%"><span>操作</span></th>
          </tr>
          <volist name="list" id="vo">
            <tr>
              <td height="25" align="center">{$vo.user_id}</td>
              <td align="center"><eq name="vo.type" value="1">购买机器人
                  <else />
                  租用机器人</eq></td>
              <td align="center">{$vo.bz}</td>
              <td align="center">{$vo.epoints}</td>
              <td align="center">{$vo.pdt|date='Y-m-d H:i:s',###}</td>
              <td align="center">
              
              <eq name="vo.type" value="2"><a href="__URL__/JJRdelform/id/{$vo.id}/">停 租</a></eq>
              <eq name="vo.type" value="3">已停租</eq>
              
              </td>
            </tr>
          </volist>
          <tr>
            <td align="center" height="25" colspan="8">{$page}</td>
          </tr>
        </table>
</td>
    </tr>
  </table>
</div>
</body></html>