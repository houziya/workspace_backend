<div class="row-fluid">
  <div class="span12">
    <h3 class="page-title">申请管理 <small>My Homepage</small> </h3>
    <ul class="breadcrumb">
      <li> <i class="icon-home"></i> <a href="__APP__/Public/main">Home</a> <i class="icon-angle-right"></i> </li>
      <li><a href="__APP__/YouZi/adminshenqing">申请管理</a></li>
    </ul>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <div class="portlet box grey">
      <div class="portlet-title">
        <div class="caption"><i class="icon-list"></i> 申请管理</div>
      </div>
      <div class="portlet-body flip-scroll">
        <table class="table table-hover table-bordered flip-content  table-full-width">
          <thead>
            <tr>
              <th nowrap >申请时间</th>
              <th nowrap >申请级别</th>
              <th nowrap >申请人</th>
              <th nowrap >操作</th>
            </tr>
          </thead>
          <tbody>
            <volist name="list" id="vo">
              <tr align="center" class="content_td">
                <td >{$vo['shijian'|date='y-m-d',###]}</td>
                <td ><switch name="vo.jibie">
                    <case value="1">H1</case>
                    <case value="2">H2</case>
                    <case value="3">H3</case>
					   <case value="4">H4</case>
                    <case value="5">H5</case>
                    <case value="6">H6</case>
                  </switch></td>
                <td ><span class="STYLE1">{$vo['uid']}</span></td>
                <td ><if condition="$vo['is_done']==0"> <a href="__URL__/adminshenqingsave/uid/{$vo['uid']}/jibie/{$vo['jibie']}/id/{$vo['id']}">
                    <input type="button" value="通过" class="btn green">
                    </a>
                    <else />
                    已经审核 </if></td>
              </tr>
            </volist>
          </tbody>
        </table>
      </div>
    </div>
    <table class="table table-hover table-bordered flip-content  table-full-width">
      <tr>
        <td><div align="center">{$page}</div></td>
      </tr>
    </table>
  </div>
</div>
