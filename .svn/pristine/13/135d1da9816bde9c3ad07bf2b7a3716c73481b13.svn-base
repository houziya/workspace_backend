.preload1 {background: url(../photo/top_bj.gif);}
.preload2 {background: url(../photo/t.gif);}

#nav {
	padding:0;
	margin:0;
	list-style:none;
	height:47px;
	background:url(../photo/top_bj.jpg);
	position:relative;
	z-index:500; line-height:47px;
	
}
#nav li.top {display:block; float:left; line-height:46px;border-right-width: 1px;
	border-right-style: solid;
	border-right-color: #333333;}
#nav li a.top_link {display:block; float:left; height:47px; line-height:47px; color:#ffffff; text-decoration:none; font-size:12px; padding:0 12px 0 12px; cursor:pointer;background: url(../photo/top_bj.jpg);}
#nav li a.top_link span {float:left; display:block; padding:0 24px 0 12px; height:47px; background: url(three_0.gif) right top no-repeat;}
#nav li a.top_link span.down {float:left; display:block; padding:0; 24px 0 12px; height:47px; background: url(../photo/top_bj.jpg) no-repeat right top;}

#nav li:hover a.top_link {
	color:#fff;
	background: url(../photo/t.gif) no-repeat;
	border-right-width: 2px;
	border-right-style: solid;
	border-right-color: #333333; 
}
#nav li:hover a.top_link span {background:url(../photo/t.gif) no-repeat right top;}
#nav li:hover a.top_link span.down {background:url(../photo/t.gif) no-repeat right top;}

/* Default list styling */

#nav li:hover {
	position:relative;
	z-index:160;
}

#nav li:hover ul.sub
{left:1px; top:47px; background: #373535; padding:3px; border:1px solid #ffffff; white-space:nowrap; width:110px; height:auto; z-index:300; }
#nav li:hover ul.sub li
{
	display:block;
	height:30px;
	position:relative;
	float:left;
	width:110px;
	font-weight:normal;
	text-align: center;
}
#nav li:hover ul.sub li a
{display:block; font-size:12px; height:30px; width:110px; line-height:30px; text-indent:5px; color:#ffffff; text-decoration:none;}
#nav li ul.sub li a.fly
{background:#373535 url(arrow.gif) 80px 6px no-repeat;}
#nav li:hover ul.sub li a:hover 
{color:#fff; border-color:#fff; background-color:#999999}
#nav li:hover ul.sub li a.fly:hover
{background:#3f96a9 url(arrow_over.gif) 80px 6px no-repeat; color:#fff;}

#nav li b {display:block; font-size:12px; height:30px; width:88px; line-height:30px; margin-bottom:3px; text-indent:6px; color:#ff6; border-bottom:1px solid #ff6; cursor:default;}

#nav li:hover li:hover ul,
#nav li:hover li:hover li:hover ul,
#nav li:hover li:hover li:hover li:hover ul,
#nav li:hover li:hover li:hover li:hover li:hover ul
{left:90px; top:-4px; background: #cccccc; padding:3px; border:1px solid #0b4d97; white-space:nowrap; width:110px; z-index:400; height:auto;}

#nav ul, 
#nav li:hover ul ul,
#nav li:hover li:hover ul ul,
#nav li:hover li:hover li:hover ul ul,
#nav li:hover li:hover li:hover li:hover ul ul
{position:absolute; left:-9999px; top:-9999px; width:0; height:0; margin:0; padding:0; list-style:none;}

#nav li:hover li:hover a.fly,
#nav li:hover li:hover li:hover a.fly,
#nav li:hover li:hover li:hover li:hover a.fly,
#nav li:hover li:hover li:hover li:hover li:hover a.fly
{background:#cccccc url(arrow_over.gif) 80px 6px no-repeat; color:#fff; border-color:#fff;} 

#nav li:hover li:hover li a.fly,
#nav li:hover li:hover li:hover li a.fly,
#nav li:hover li:hover li:hover li:hover li a.fly
{background:#cccccc url(arrow.gif) 80px 6px no-repeat; color:#000; border-color:#cccccc;} 
