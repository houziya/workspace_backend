@charset "utf-8";
/* CSS Document */

@charset "utf-8";
body { font-family: "微软雅黑", "ff-tisa-web-pro-1", "ff-tisa-web-pro-2", "Lucida Grande", "Hiragino Sans GB", "Hiragino Sans GB W3"; margin: 0px; padding: 0px; background-color: #F0F0F0; }
input { font-family: "微软雅黑", "ff-tisa-web-pro-1", "ff-tisa-web-pro-2", "Lucida Grande", "Hiragino Sans GB", "Hiragino Sans GB W3"; height:30px; }
textarea { font-family: "微软雅黑", "ff-tisa-web-pro-1", "ff-tisa-web-pro-2", "Lucida Grande", "Hiragino Sans GB", "Hiragino Sans GB W3"; }
body a { text-decoration: none; color: inherit; }
#login { position: absolute; top: 0; left: 0; height: 100%; width: 100%; background: url('../Images/login_bg.jpg'); background-position: center 0; background-repeat: no-repeat; background-attachment: fixed; background-size: cover; -webkit-background-size: cover;/* 兼容Webkit内核浏览器如Chrome和Safari */ -o-background-size: cover;/* 兼容Opera */ zoom: 1; }
.login_form { width: 420px; background: url('../Images/login_form_bg.png'); position: fixed; top: 50%; left: 50%; margin: -275px 0 0 -210px; }
.login { width: 316px; float: left; line-height: 107px; cursor: pointer; display: block; margin-left: 52px; }
.login_nav { width: 316px; float: left; margin-left: 52px; margin-top: 0px; height: 40px; line-height: 40px; text-align: center; }
.login_nav ul { list-style: none; margin: 0px; padding: 0px; float: left; height: 40px; }
.login_nav ul li { width: 158px; list-style: none; margin: 0px; padding: 0px; float: left; height: 36px; line-height: 36px; text-align: center; border-top: 4px solid #000; color: #999; font-size: 14px; cursor: pointer; }
.login_nav ul li.active { border-top: 4px solid #1985CE; color: #FFF; background: #333; }
.login_con { width: 316px; float: left; margin-left: 52px; margin-top: 0px; }
.login_con ul { list-style: none; margin: 0px; padding: 0px; float: left; width: 316px; }
.login_con ul li { list-style: none; margin-top: 10px; height: 42px; color: #999; font-size: 14px; float: left; width: 316px; }
.login_con ul li.input-error { margin-top: 0px; }
.login_con ul li input { border: 1px solid #CCC; border-radius: 4px; width: 294px; height: 40px; line-height: 40px; padding: 0px 10px; }
.login_con ul li input.login-checkbox { width: 20px; height: 20px; float: left; margin: 0px 2px; }
.login_con ul li.login_link { height: 20px; line-height: 20px; }
.login_con ul li.login_link span.span-left { float: left; }
.login_con ul li.login_link span.span-right { float: right; }
.login_con ul li a.login_sub { width: 316px; border-radius: 5px; height: 42px; line-height: 42px; background: #1985CE; display: block; text-align: center; color: #FFF; }
.login_con ul li.login_reg { text-align: center; margin-top: 32px; }
.login_con ul li.login_reg a { color: #1985CE; }
#registers { position: absolute; top: 0; left: 0; height: 100%; width: 100%; background: url('../Images/login_bg.jpg'); background-position: center 0; background-repeat: no-repeat; background-attachment: fixed; background-size: cover; -webkit-background-size: cover;/* 兼容Webkit内核浏览器如Chrome和Safari */ -o-background-size: cover;/* 兼容Opera */ zoom: 1; }
.register_form { width: 420px; background: url('../Images/register_form_bg.png') no-repeat; position: absolute; top: 50%; left: 50%; margin: -430px 0px 0px -210px; }
.register { width: 316px; float: left; line-height: 107px; cursor: pointer; display: block; margin-left: 52px; }
.register_nav { width: 316px; float: left; margin-left: 52px; margin-top: 105px; height: 40px; line-height: 40px; text-align: center; }
.register_nav ul { list-style: none; margin: 0px; padding: 0px; float: left; height: 40px; }
.register_nav ul li { width: 158px; list-style: none; margin: 0px; padding: 0px; float: left; height: 36px; line-height: 36px; text-align: center; border-top: 4px solid #000; color: #999; font-size: 14px; cursor: pointer; }
.register_nav ul li.active { border-top: 4px solid #1985CE; color: #FFF; background: #333; }
.register_con { width: 368px; height: 575px; overflow-y: auto; float: left; margin-left: 52px; margin-top: 10px; padding-bottom: 100px; margin-bottom: 20px; }
.register_con ul { list-style: none; margin: 0px; padding: 0px; float: left; width: 316px; }
.register_con ul li { list-style: none; margin-top: 20px; color: #999; font-size: 14px; float: left; width: 316px; }
.register_con ul li.input-error { margin-top: 0px; }
.input-error span { height: 24px; line-height: 24px; }
.input-error span i { color: #F00; margin-right: 5px; font-style: normal; }
.register_con ul li input { border: 1px solid #CCC; border-radius: 4px; width: 294px; height: 40px; line-height: 40px; padding: 0px 10px; }
.register_con ul li input.register-checkbox { width: 20px; height: 20px; float: left; margin: 0px 2px; }
.register_con ul li.register_link { height: 20px; line-height: 20px; }
.register_con ul li.register_link span.span-left { float: left; }
.register_con ul li.register_link span.span-right { float: right; }
.register_con ul li a.register_sub { width: 316px; border-radius: 5px; height: 42px; line-height: 42px; background: #1985CE; display: block; text-align: center; color: #FFF; }
.register_con ul li.register_reg { text-align: center; margin-top: 10px; margin-bottom: 20px; }
.register_con ul li.register_reg a { color: #1985CE; }
.alert-success, .alert-danger, .alert-info, .alert-warning { border: medium none; }
.alert-success { color: #468847; background-color: #DFF0D8; border-color: #D6E9C6; }
.alert-error { color: #fff; background-color: #CC6666; border-color: #FBEED5; }
.alert { padding: 15px; margin-bottom: 20px; border: 1px solid transparent; /*border-radius: 4px;*/ }
.visible { display: block; }
.input-error-info { float: left; width: 316px; margin: 0px; padding: 0px; height: 22px; line-height: 22px; }
