<div class="row-fluid">
  <div class="span12">
    <h3 class="page-title">奖金查询 <small>My Homepage</small> </h3>
    <ul class="breadcrumb">
      <li> <i class="icon-home"></i> <a href="__APP__/Public/main">Home</a> <i class="icon-angle-right"></i> </li>
      <li><a href="__APP__/Bonus/adminFinanceTable">奖金查询</a></li>
    </ul>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <div class="portlet box grey">
      <div class="portlet-title">
        <div class="caption"><i class="icon-list"></i> 奖金查询</div>
      </div>
      <div class="portlet-body flip-scroll">
        <table class="table table-hover table-bordered flip-content  table-full-width">
          <thead>
            <tr>
              <th><span>{:xstr('clear_date_time')}</span></th>
              <th {$c_b[1]}><span>{$fee_s7[0]}</span></th>
              <th {$c_b[2]}><span>{$fee_s7[1]}</span></th>
              <th {$c_b[3]}><span>{$fee_s7[2]}</span></th>
              <th {$c_b[4]}><span>{$fee_s7[3]}</span></th>
              <th {$c_b[5]}><span>{$fee_s7[4]}</span></th>
              <th {$c_b[6]}><span>{$fee_s7[5]}</span></th>
              <th {$c_b[7]}><span>{$fee_s7[6]}</span></th>
              <th {$c_b[8]}><span>{$fee_s7[7]}</span></th>
              <th {$c_b[9]}><span>{$fee_s7[8]}</span></th>
              <th {$c_b[10]}><span>{$fee_s7[9]}</span></th>
              <th {$c_b[11]}><span>{$b_b[11]}</span></th>
              <th {$c_b[0]}><span>{$b_b[0]}</span></th>
              <th {$c_b[12]}><span>{$b_b[12]}</span></th>
            </tr>
          </thead>
          <tbody>
            <volist name='list' id='vo'>
              <tr align="center">
                <td>{$vo['e_date']|date='Y-m-d H:i:s',###}</td>
                <td {$c_b[1]}>{$vo['b1']}</td>
                <td {$c_b[2]}>{$vo['b2']}</td>
                <td {$c_b[3]}>{$vo['b3']}</td>
                <td {$c_b[4]}>{$vo['b4']}</td>
                <td {$c_b[5]}>{$vo['b5']}</td>
                <td {$c_b[6]}>{$vo['b6']}</td>
                <td {$c_b[7]}>{$vo['b7']}</td>
                <td {$c_b[8]}>{$vo['b8']}</td>
                <td {$c_b[9]}>{$vo['b9']}</td>
                <td {$c_b[10]}>{$vo['b10']}</td>
                <td {$c_b[11]}>{$vo['b1']+$vo['b2']}</td>
                <td {$c_b[0]}>{$vo['b0']}</td>
                <td {$c_b[12]}><a href="__URL__/adminFinanceTableShow/did/{$vo['did']}/" title="{:xstr('tpl057')}">{:xstr('view')}</a></td>
              </tr>
            </volist>
            <tr align="center">
              <td>{:xstr('total3')}：</td>
              <td {$c_b[1]}>{$count[1]}</td>
              <td {$c_b[2]}>{$count[2]}</td>
              <td {$c_b[3]}>{$count[3]}</td>
              <td {$c_b[4]}>{$count[4]}</td>
              <td {$c_b[5]}>{$count[5]}</td>
              <td {$c_b[6]}>{$count[6]}</td>
              <td {$c_b[7]}>{$count[7]}</td>
              <td {$c_b[8]}>{$count[8]}</td>
              <td {$c_b[9]}>{$count[9]}</td>
              <td {$c_b[10]}>{$count[10]}</td>
              <td {$c_b[11]}>{$count[1]+$count[2]}</td>
              <td {$c_b[0]}>{$count[0]}</td>
              <td {$c_b[12]}>&nbsp;</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
    <table class="table table-hover table-bordered flip-content  table-full-width">
      <tr>
        <td><form id="form1" name="form1" method="post" action="__URL__/adminFinanceTable">
            {:xstr('bonus_date')}：
            <input name="FanNowDate" type="text" id="FanNowDate" onFocus="showCalendar(this)" style="width:10%;" readonly />
            <input type="submit" name="Submit" value="{:xstr('query')}" class="btn red" />
            &nbsp;&nbsp;
            <input name="button3" type="button" onclick="window.location.href='__URL__/financeDaoChu_JJCX/p/{$PP}/'" value="{:xstr('export_to_excel')}" class="btn red" />
          </form></td></tr>
        <tr><td>{$page}</td>
      </tr>
    </table>
    <table class="table table-hover table-bordered flip-content  table-full-width">
      <tr>
        <td><form id="form1" name="form1" method="post" action="__APP__/Bonus/financeTable">
            {:xstr('account')}：
            <input name="UserID" type="text"id="UserID" title="{:xstr('account_query')}" maxlength="200" >
            <input type="submit" name="Submit" value="{:xstr('search')}" class="btn red"/>
          </form></td>
      </tr>
    </table>
  </div>
</div>
