<include file="../Public/header" />
<style>
h4{ margin-left:18%;}
.Agreement{width:90%; height:100px; overflow:auto; border:1px solid #7F9DB9; padding:2px; font-size:12px; line-height:24px; margin:10px 0px;}
</style>
<div class="ncenter_box">
<div class="accounttitle"><h1>{:xstr('tpl098')}</h1></div>
<form action="__URL__/eb_list_done" method="post" name="form1" id="form1">
<table width="100%" class="list" border="0" cellpadding="3" cellspacing="0">
	<tr>
	  <td width="20%" height="30" align="right" class="tabr">{:xstr('money')}：</td>
	  <td width="80%">{$rs['money']}</td>
	  </tr>
	<tr>
	  <td height="30" align="right" class="tabr">{:xstr('tpl091')}：</td>
	  <td>￥ {$rs['money_two']}</td>
	  </tr>
	<tr>
	  <td height="30" class="tabr">&nbsp;</td>
	  <td><label>
	    <input name="agree" type="checkbox" id="agree" value="1" checked="checked" onpropertychange="change_fc()"/>
	    {:xstr('tpl109')}</label></td>
	  </tr>
    <tr>
	  <td height="30" align="right" class="tabr">{:xstr('verify_code')}：</td>
	  <td><input name="verify" id="verify" type="text" size="8" maxlength="4" />
      <img src="__APP__/Public/verify/" border="0" alt="{:xstr('click_to_reflush_verify_code')}" id="verifyImg" onclick="fleshVerify()" style="cursor:pointer" align="absmiddle" />&nbsp;</td>
	</tr>
	<tr>
	  <td height="30" align="right" class="tabr">{:xstr('second_password')}：</td>
	  <td><input name="uspass" type="password" class="ipt" id="uspass" value="" maxlength="20"/></td>
	</tr>
    <tr>
	  <td height="30"><input name="cid" type="hidden" id="cid" value="{$rs['id']}" /></td>
	  <td>
      <input type="submit" name="Submit1" value="{:xstr('done')}" class="button" id="Submit1" />
      <input name="backb" type="button" class="button3" value="{:xstr('go_back')}" id="backb" onclick="history.back();"/>
      </td>
	</tr>
</table>
</form>
</div></div>
</body>
</html>
<script language="JavaScript">
function change_fc(){
	if(document.form1.agree.checked==true){
		document.form1.Submit1.disabled=false;
	}else{
		document.form1.Submit1.disabled=true;
	}
}

