@charset "utf-8";
/* CSS Reset */
body,div,dl,dt,dd,ul,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td { margin:0;padding:0;}
fieldset,img { border:0;}
address,caption,cite,code,dfn,em,strong,th,var {font-style:normal;	font-weight:normal;}
ul {list-style:none;}
caption,th {text-align:left;}
h1,h2,h3,h4,h5,h6 {font-size:100%;font-weight:normal;}
q:before,q:after {content:'';}
abbr,acronym { border:0;}
a { text-decoration:none; color:#335cb8;cursor:pointer;}
a:hover {color:#F00;}
.clear { clear:both; font-size:0;}
.box { padding:12px;}
.m10 { margin:2px 0;}
.bar_foot { text-align:center; line-height:24px;}

/* Main */
