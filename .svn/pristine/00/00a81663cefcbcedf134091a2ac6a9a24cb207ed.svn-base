<div class="ncenter_box">

    <form method="post" name="match" action="__APP__/YouZi/quxiaopipeis">
        <table width="100%" border="0" cellspacing="10" cellpadding="0">

            <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tab11 tab_bor">
                <h2>取消匹配</h2>
                <tr>
                    <td width="100%" height="20">
                        进场订单编号:<input type="text" name="jujue" style=" background-color: #ADD9C0;"/>&nbsp;&nbsp;输入进场方订单，两方同时恢复原始状态

                    </td>


                </tr>
                <tr>
                    <td><input type="submit" name="action" value="取消订单" class="button_text"
                               onclick="if(confirm('确定要取消订单吗?')) return true; else return false;">&nbsp;&nbsp;</td>
                </tr>

            </table>
    </form>

    <form method="post" name="match" action="__APP__/YouZi/cancelMatchsOutOrder">
        <table width="100%" border="0" cellspacing="10" cellpadding="0">

            <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tab11 tab_bor">
                <h2>取消匹配</h2>
                <tr>
                    <td width="100%" height="20">
                        出场订单编号:<input type="text" name="out_order" style=" background-color: #ADD9C0;"/>&nbsp;&nbsp;输入出场方订单，只恢复出场订单

                    </td>


                </tr>
                <tr>
                    <td><input type="submit" name="action" value="取消订单" class="button_text"
                               onclick="if(confirm('确定要取消订单吗?')) return true; else return false;">&nbsp;&nbsp;</td>
                </tr>

            </table>
    </form>
</div>
