<include file="../Public/header" />
<div class="ncenter_box">
<div class="accounttitle"><h1>开通账户 </h1></div>
<form name="form3" method="post" action="__URL__/menberAC">
<table width="100%" class="tab3" border="0" cellpadding="3" cellspacing="1" id="tb1" bgcolor="#b9c8d0">
	<thead>
    <tr align="center">
        <th></th>
        <th><span>{:xstr('account')}</span></th>
        <th><span>{:xstr('bank_name')}</span></th>
        <th><span>{:xstr('bank_code')}</span></th>
        <th><span>{:xstr('bank_user_name')}</span></th>
        <th><span>{:xstr('contact_phone')}</span></th>
        <th><span>{:xstr('reg_date_time')}</span></th>
        <th><span>{:xstr('open_date_time')}</span></th>
        <th><span>{:xstr('reg_level')}</span></th>
        <th><span>{:xstr('reg_money')}</span></th>
        <th><span>{:xstr('status')}</span></th>
    </tr>
    </thead>
    <volist name="list" id="vo">
    <tr align="center">
        <td> <input type="checkbox" name="tabledb[]" value="{$vo['id']}" class="btn2" /></td>
        <td><!--<a href="__URL__/menberData/bj_id/{$vo['id']}/eth/menber" title="{:xstr('detailed_information')}">-->{$vo['user_id']}</a></td>
        <td>{$vo['bank_name']}</td>
        <td>{$vo['bank_card']}</td>
        <td>{$vo['user_name']}</td>
        <td>{$vo['user_tel']}</td>
        <td>{$vo.rdt|date='Y-m-d H:i:s',###}</td>
        <td>{$vo.pdt|date='Y-m-d H:i:s',###}</td>
        <td>{$voo[$vo['u_level']]}</td>
        <!--<td>{$vo["u_level"]+1}</td>-->
        <td>{$vo['cpzj']}</td>
        <td><eq name="vo['is_pay']" value='0'><span style="color: #FF0000;">{:xstr('not_opened')}</span><else />{:xstr('is_opened')}</eq></td>
    </tr>
    </volist>
</table>
<table width="100%" class="tab3_bottom" border="0" cellpadding="0" cellspacing="1">
    <tr>
    	<td width="50%"><input name="{:xstr('select_all')}" type="button" class="button_text" id="{:xstr('select_all')}" onclick="CheckAll(this.form)" value="{:xstr('select_all')}" />	  &nbsp;&nbsp;
          <input type="submit" name="action" value="{:xstr('open_account')}" onclick="if(confirm('{:xstr('tpl024')}')) return true; else return false;" class="button_text">&nbsp;&nbsp;
          <input type="submit" name="action" value="{:xstr('delete_account')}" onclick="if(confirm('{:xstr('tpl025')}')) return true; else return false;" class="button_text"></td>

        <td width="50%" align="center">{$page}</td>
    </tr>
</table>
</form>
<table width="600" align="center">
    <tr>
      <td>&nbsp;</td>
    </tr>
    <tr>
    <td align="center"><form method='post' action="__URL__/menber/">{:xstr('search_account')}：<input type="text" name="UserID" title="{:xstr('account_query')}">
    <input type="submit" name="Submit" value="{:xstr('query')}"  class="button_text"/><!--&nbsp;&nbsp;您的代理账户金额：<span class="hong">{$frs['agent_cash']}</span>-->
    </form></td>
    </tr>
    <tr>
      <td align="center" colspan="2">
    <img src="__PUBLIC__/Images/money_lit.png" align="absmiddle"/>&nbsp;&nbsp;&nbsp;报单币:&nbsp;<span class="hong">{$frs['agent_cash']}</span></td></tr>
</table>
</div>
</body>
</html>
<script>new TableSorter("tb1");</script>