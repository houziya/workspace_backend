<include file="../Public/header" />
<style>
.tab_l3 {
	font-size:14px;
}
</style>
<div class="ncenter_box">
  <div class="accounttitle">
    <h1>下属收益记录</h1>
  </div>
  <table width="100%" border="0" cellspacing="0" cellpadding="5" class="tab3" id="tb2">
    <tr>
      <th width="14%" height="28"><span>{:xstr('account')}</span></th>
      <th width="12%">收益账户</th>
      <th width="12%"><span>{:xstr('money')}</span></th>
      <th width="12%"><span>{:xstr('time')}</span></th>
      <th width="12%"><span>{:xstr('remark')}</span></th>
    </tr>
    <volist name="list" id="vo">
      <tr>
        <td height="25" align="center">{$vo.user_id}</td>
        <td align="center">{$vo.user_did}</td>
        <td align="center">{$vo.epoints}</td>
        <td align="center">{$vo.pdt|date='Y-m-d H:i:s',###}</td>
        <td align="center" nowrap="nowrap">{$vo.bz}</td>
      </tr>
    </volist>
    <tr>
      <td align="center" height="25" colspan="10">{$page}</td>
    </tr>
  </table>
 
</div>
</body></html>